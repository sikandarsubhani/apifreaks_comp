{
  "name": "@7sikandar/af-nav",
  "version": "0.1.0",
  "private": false,
  "description": "API Freaks Components",
  "author": "Sikandar Subhani ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sikandarsubhani/apifreaks_comp.git"
  },
  "keywords": [
    "api",
    "components",
    "react",
    "nextjs"
  ],
  "main": "dist/exports.cjs",
  "module": "dist/exports.mjs",
  "types": "dist/exports.d.ts",
  "style": "dist/af-components.css",
  "exports": {
    ".": {
      "types": "./dist/exports.d.ts",
      "import": "./dist/exports.mjs",
      "require": "./dist/exports.cjs"
    },
    "./styles": "./dist/af-components.css",
    "./styles/af-components.css": "./dist/af-components.css",
    "./styles/globals.css": "./dist/globals.css",
    "./styles/blog.css": "./dist/blog.css",
    "./styles/jsoneditor.css": "./dist/jsoneditor.css",
    "./styles/public-pages.css": "./dist/public-pages.css",
    "./styles/index.css": "./dist/index.css",
    "./tailwind.config": "./tailwind.config.js"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:css": "node build-css.mjs",
    "build:package": "tsup && npm run build:css && cp tailwind.config.js dist/",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build:package && npm pack"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/tryghost__content-api": "^1.3.17",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "postcss-cli": "^11.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tsup": "^8.5.0",
    "typescript": "^5"
  },
  "files": [
    "dist",
    "public"
  ]
}
